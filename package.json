{
    "name": "ifc-syntax",
    "displayName": "IFC Syntax",
    "description": "Extension that brings IFC (Industry Foundation Classes) support to VSCode",
    "repository": "https://github.com/AlanRynne/ifc-syntax-highlight.git",
    "icon": "images/IFC-Syntax-Icon.png",
    "version": "0.0.1",
    "publisher": "AlanRynne",
    "engines": {
        "vscode": "^1.36.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "ifc",
                "aliases": [
                    "IFC",
                    "ifc"
                ],
                "extensions": [
                    ".ifc"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "ifc",
                "scopeName": "source.ifc",
                "path": "./syntaxes/ifc.tmLanguage.json"
            }
        ]
    }
}